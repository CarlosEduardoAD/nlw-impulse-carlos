// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
// Mudan√ßa da base de dados sqlite para postgres

  provider = "postegresql"
  url      = env("DATABASE_URL")
}

model Feedback {
  id String @id @default(uuid())
  type String
  comment String
  screenshot String?

  @@map("feedbacks")
}